{
  "directory_name": "latex-makefile-boilerplate",
  "document_name": "my-document",
  "editor": [
    "VS Code with LaTeX Workshop extension",
    "Not specified"
  ],
  "indent": [
    "2 spaces",
    "4 spaces",
    "8 spaces",
    "Tab"
  ],
  "formatter": [
    "latexindent",
    "None"
  ],
  "linter": [
    "ChkTeX",
    "None"
  ],
  "_makefile_version": "0.13.0",
  "_jinja2_env_vars": {
    "lstrip_blocks": true,
